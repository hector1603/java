package com.krakedev;

public class TestResultado {

	public static void main(String[] args) {
		Rectangulo rec1 = new Rectangulo();
		Rectangulo rec2 = new Rectangulo();

		rec1.base = 10;
		rec1.altura = 5;

		int resultRec1 = rec1.calcularArea();
		System.out.println("Rectangulo 1: " + resultRec1);

		rec2.base = 8;
		rec2.altura = 3;
		int resultRec2 = rec2.calcularArea();
		System.out.println("Rectangulo 2: " + resultRec2);
	}

}
