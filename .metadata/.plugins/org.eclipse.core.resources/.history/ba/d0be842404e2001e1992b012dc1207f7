package com.krakedev;

public class Fecha {
	private int anio;
	private int mes;
	private int dia;
	
	public int getAnio() {
		return anio;
	}
	public void setAnio(int nuevoAnio) {
		anio = nuevoAnio;
	}
	
	public int getMes() {
		return mes;
	}
	public void setMes(int nuevoMes) {
		mes = nuevoMes;
	}
	
	public int getDia() {
		return dia;
	}
	public void setDia(int nuevoDia) {
		dia = nuevoDia;
	}	
}
